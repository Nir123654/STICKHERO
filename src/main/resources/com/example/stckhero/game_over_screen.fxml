      <Button fx:id="restartButton" layoutX="484.0" layoutY="579.0" mnemonicParsing="false" onAction="#onRestartButtonClick" opacity="0.0" style="-fx-min-height: 100; -fx-min-width: 100; -fx-max-width: 100; -fx-max-height: 100;" />
