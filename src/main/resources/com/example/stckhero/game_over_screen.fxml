<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stckhero.GameOverScreenController">
   <children>
      <ImageView fx:id="titleimage1" fitHeight="800.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
